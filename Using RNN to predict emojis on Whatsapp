        "# predicting time (lets see how our model predicts to new data)\n",
        "\n",
        "def predict_emoji(text):\n",
        "  seq= tokenizer.texts_to_sequences([text])\n",
        "  pad=pad_sequences(seq, maxlen=10, padding='post')\n",
        "\n",
        "\n",
        "  pred= our_model.predict(pad)\n",
        "\n",
        "  index = np.argmax(pred)\n",
        "\n",
        "  predicted_emoji = number_to_emoji[index]\n",
        "\n",
        "  return predicted_emoji"
