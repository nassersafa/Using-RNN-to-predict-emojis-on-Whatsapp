        "\n",
        "    ],\n",
        "    \"sad\": [\n",
        "        \"I'm feeling very down\",\n",
        "        \"Today was terrible\",\n",
        "        \"I feel so lonely\",\n",
        "        \"Nothing is going right\",\n",
        "        \"I'm exhausted emotionally\"\n",
        "    ],\n",
        "    \"love\": [\n",
        "        \"I miss you so much\",\n",
        "        \"Sending you lots of love\",\n",
        "        \"You mean the world to me\",\n",
        "        \"I care about you deeply\",\n",
        "        \"I canâ€™t stop thinking about you\"\n",
        "    ],\n",
        "    \"funny\": [\n",
        "        \"This is hilarious\",\n",
        "        \"I can't stop laughing\",\n",
        "        \"Bro what are you doing\",\n",
        "        \"This joke killed me\",\n",
        "        \"Iâ€™m crying from laughing\"\n",
        "    ],\n",
        "    \"angry\": [\n",
        "        \"I'm so mad right now\",\n",
        "        \"This is so frustrating\",\n",
        "        \"I can't believe this happened\",\n",
        "        \"Why does this always happen\",\n",
        "        \"I'm annoyed\"\n",
        "    ],\n",
        "    \"tired\": [\n",
        "        \"I'm so tired today\",\n",
        "        \"Can't keep my eyes open\",\n",
        "        \"I need sleep\",\n",
        "        \"Exhausted after work\",\n",
        "        \"Long day ahead\"\n",
        "    ]\n",
        "\n",
        "\n",
        "}\n",
        "\n",
        "emoji_expression= {\n",
        "    'happy': \"ğŸ˜Š\",\n",
        "    \"sad\": \"ğŸ˜¢\",\n",
        "    \"love\": \"â¤ï¸\",\n",
        "    \"funny\": \"ğŸ˜‚\",\n",
        "    \"angry\": \"ğŸ˜¡\",\n",
        "    \"tired\": \"ğŸ˜´\"\n",
        "}\n",
        "\n",
        "\n",
        "\n",
        "# now let us generate the dataset\n",
        "\n",
        "data = []\n",
        "for i in range(500):\n",
        "  emoji = random.choice(list(emoji_expression.keys()))\n",
        "  text = random.choice(whatsapp_previous_messages[emoji])\n",
        "  realemoji = emoji_expression[emoji]\n",
        "  data.append([text, realemoji ])\n",
        "\n",
        "\n",
        "df = pd.DataFrame(data,columns= [\"text\", 'emoji'])\n",
        "df.head()\n",
        "\n",
        "\n",
        "df.shape"
